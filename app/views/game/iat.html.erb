<%= render "unity", game: 'IAT-' + @version %>

<div class="main_content">
  <div class="mc_header">
      <h1>Implicit Association Test (IAT)</h1>
  </div>

  <div id="unityPlayer">
    <div class="missing">
      'asset_path'
      <a href="http://unity3d.com/webplayer/" title="Unity Web Player. Install now!">
        <img alt="Unity Web Player. Install now!" src="http://webplayer.unity3d.com/installation/getunity.png" width="193" height="63" />
      </a>
    </div>
  </div>

  <ul class="bias_buttons center">
  <li>

  <% if @version == "A" %>
    <%= link_to game_path do %>
      <span >Continue</span>
    <% end %>
  <% else %>
    <%= link_to complete_path do %>
      <span >Continue</span>
    <% end %>
  <% end %>
  </li>
  </ul>
</div>
<%= render "unity", game: 'IAT-' + @version %>

<div class="main_content">
  <div class="mc_header">
    <h1>Implicit Association Test (IAT)</h1>

    <% if flash[:notice] %>
        <div id="notice"><%= flash[:notice] %></div>
    <% end %>
  </div>

  <div id="unityPlayer">
    <div class="missing">
      'asset_path'
      <a href="http://unity3d.com/webplayer/" title="Unity Web Player. Install now!">
        <img alt="Unity Web Player. Install now!" src="http://webplayer.unity3d.com/installation/getunity.png" width="193" height="63" />
      </a>
    </div>
  </div>

  <div id="player_buttons">
    <% if @version == "Pre" %>
      <span class="bias_button">
        <%= link_to "Continue", game_path %>
      </span>
    <% else %>
      <span class="bias_button">
        <%= link_to "Continue", complete_path %>
      </span>
    <% end %>
  </div>
</div>